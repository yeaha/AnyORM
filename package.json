{
  "name": "anyorm",
  "author": "yangyi.cn.gz@gmail.com",
  "version": "0.2.13",
  "description": "Object Relational Mapper for node.js",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/yeaha/AnyORM.git"
  },
  "keywords": [
    "DataMapper",
    "ORM",
    "MySQL",
    "PostgreSQL"
  ],
  "license": "MIT",
  "scripts": {
