    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/faker": "^4.1.0",
    "@types/lodash": "^4.14.63",
    "@types/node": "^7.0.14",
    "ava": "^0.19.1",
    "faker": "^4.1.0",
    "source-map-support": "^0.4.14"
  },
  "ava": {
    "files": [
      "build/tests/**/*.js",
