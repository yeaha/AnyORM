{
    "extends": "tslint:recommended",
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-classes-per-file": [
            true,
            10
        ],
        "no-empty": false,
        "no-string-literal": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false
    }
}