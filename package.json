  },
  "peerDependencies": {
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "@types/any-db": "^2.1.30",
    "@types/any-db-transaction": "^2.2.30",
    "@types/knex": "^0.0.48",
    "immutable": "^4.0.0-rc.2",
