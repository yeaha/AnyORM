{
    "extends": "tslint:recommended",
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-classes-per-file": [
            true,
            10
        ],
        "member-access": false,
        "newline-before-return": true,
        "no-empty": false,
        "no-string-literal": false,
        "no-string-throw": true,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ]
    }
}
